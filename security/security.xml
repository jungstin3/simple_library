<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="1">
        <record id="module_category_sp" model="ir.module.category">
            <field name="name">Manajemen Perpustakaan</field>
            <field name="description">Akses untuk modul Manajemen Perpustakaan</field>
            <field name="sequence">10</field>
        </record>

        <record id="group_sp_user" model="res.groups">
            <field name="name">User SP</field>
            <field name="category_id" ref="simple_library.module_category_sp"/>
        </record>

        <record id="group_sp_admin" model="res.groups">
            <field name="name">Administrator SP</field>
            <field name="category_id" ref="simple_library.module_category_sp"/>
            <field name="implied_ids" eval="[
                (4, ref('simple_library.group_sp_user')),
                (4, ref('base.group_system'))
            ]"/>
        </record>

        <record id="ir_sequence_group_sp_user" model="ir.model.access">
            <field name="name">ir.sequence group_sp_user</field>
            <field name="model_id" ref="base.model_ir_sequence"/>
            <field name="group_id" ref="simple_library.group_sp_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="0"/>
        </record>
    </data>   
</odoo>